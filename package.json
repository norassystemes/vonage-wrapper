{
  "name": "vonage-wrapper",
  "license": "MIT",
  "version": "0.0.1",
  "description": "A wrapper for the Vonage API",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vitest && ts-node src/playground.ts",
    "test": "vitest run",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "lint": "tsc",
    "ci": "pnpm run lint && pnpm run test && pnpm run build",
    "release": "pnpm run lint && pnpm run test && pnpm run build && changeset publish"
  },
  "peerDependencies": {
    "@vonage/auth": "^1.1.1",
    "@vonage/server-sdk": "^3.1.1",
    "abstract-logging": "^2.0.1",
    "dayjs": "^1.11.7",
    "pino": "^8.11.0",
    "pino-pretty": "^9.4.0",
    "zod": "^3.21.1",
    "zod-error": "^1.5.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/node": "^18.14.6",
    "@vonage/auth": "^1.1.1",
    "@vonage/server-sdk": "^3.1.1",
    "abstract-logging": "^2.0.1",
    "dayjs": "^1.11.7",
    "pino": "^8.11.0",
    "pino-pretty": "^9.4.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2",
    "zod": "^3.21.1",
    "zod-error": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/norassystemes/vonage-wrapper"
  },
  "author": {
    "name": "Noras"
  },
  "keywords": [
    "vonage",
    "conversation-api",
    "wrapper"
  ],
  "bugs": {
    "url": "https://github.com/norassystemes/vonage-wrapper/issues"
  },
  "homepage": "https://github.com/norassystemes/vonage-wrapper#readme",
  "engines": {
    "node": ">=14"
  }
}
